<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title><%= title %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="stylesheet" 
        href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" 
        integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" 
        crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" media="screen" href="../stylesheets/style.css" />
  <link rel="stylesheet" type="text/css" media="screen" href="../stylesheets/events.css" />
  <link href="https://fonts.googleapis.com/css?family=Gochi+Hand|Rubik|Lato" rel="stylesheet">
</head>
<body>

  <%- include _nav.ejs %>

  <main id="main-content" class="container-fluid fill-screen">
    <div class="row d-flex justify-content-center mt-5">
      <div class="col-8 mrl-auto">
        <h4>Edit Profile</h4>
        <form method="">
          <div class="form-control">

            <label for="first" class="mt-3">First Name</label>
            <input type="text" class="form-control mb-3" id="first">

            <label for="last">Last Name</label>
            <textarea type="text" class="form-control mb-3" id="last"></textarea>

            <label for="username">Username</label>
            <input type="email" class="form-control mb-3" id="username">

            <label for="location">Location</label>
            <input type="text" class="form-control mb-3" id="location">

            <label for="age">Age</label>
            <input type="number" class="form-control mb-3" id="age">

            <label for="photo">Profile Photo</label>
            <input type="file" class="form-control-file mb-3" id="photo">

            <hr>
            <button type="submit" class="btn btn-warning mb-3">Save Changes</button>
            
          </div>
        </form>
      </div>
    </div>
  </main>

<%- include _footer.ejs %>

<script
