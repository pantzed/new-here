  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title><%= title %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="google-site-verification" content="40grDVhUWx2WjOFQs7zHabDNnZYqb6UXmfZ2Bt7OyGU" />
  <meta name="google-signin-scope" content="profile email">
  <meta name="google-signin-client_id" content="1074020877158-eb30cjftrhcvulod09gqgtbnbjqj7srg.apps.googleusercontent.com">
  <link rel="stylesheet" 
        href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" 
